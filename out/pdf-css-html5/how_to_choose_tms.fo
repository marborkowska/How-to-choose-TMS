                                        <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman,Liberation Serif,DejaVu Serif,Symbol,Segoe UI Symbol,FreeSerif,ZapfDingbats"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline color="rgb(97 , 168 , 223)"><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.trados.com/products/trados-studio/">Trados
                                                Studio</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.trados.com/products/trados-studio/"><fo:inline font-style="italic"/></fo:basic-link></fo:inline></fo:block></fo:list-item-body></fo:list-item>
                                        <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman,Liberation Serif,DejaVu Serif,Symbol,Segoe UI Symbol,FreeSerif,ZapfDingbats"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline color="rgb(97 , 168 , 223)"><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.memoq.com/">memoQ</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.memoq.com/"><fo:inline font-style="italic"/></fo:basic-link></fo:inline></fo:block></fo:list-item-body></fo:list-item>
                                        <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman,Liberation Serif,DejaVu Serif,Symbol,Segoe UI Symbol,FreeSerif,ZapfDingbats"></fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:inline color="rgb(97 , 168 , 223)"><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.wordfast.com/">Wordfast</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" external-destination="https://www.wordfast.com/"><fo:inline font-style="italic"/></fo:basic-link></fo:inline>
